s [[/r 4d6[mental]]] damage, depending on its basic Will save.</li>\n</ul>"},"source":{"value":""},"rules":[],"slug":null,"schema":{"version":0.819,"lastMigration":null},"traits":{"value":["manipulate"],"custom":"","rarity":"common"},"actionType":{"value":"action"},"actionCategory":{"value":"offensive"},"actions":{"value":1},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"weapon":{"value":""}},"type":"action","effects":[],"folder":null,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"3zLzmwjB7sNBA9KG","img":"systems/pf2e/icons/default-icons/lore.svg","name":"Acrobatics","sort":2000000,"system":{"description":{"value":""},"source":{"value":""},"rules":[],"slug":null,"schema":{"version":0.819,"lastMigration":null},"mod":{"value":13},"proficient":{"value":0},"traits":{"custom":"","rarity":"common","value":[]}},"type":"lore","effects":[],"folder":null,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"GTOFboobs9hUeL9A","img":"systems/pf2e/icons/default-icons/lore.svg","name":"Deception","sort":2100000,"system":{"description":{"value":""},"source":{"value":""},"rules":[],"slug":null,"schema":{"version":0.819,"lastMigration":null},"mod":{"value":11},"proficient":{"value":0},"traits":{"custom":"","rarity":"common","value":[]}},"type":"lore","effects":[],"folder":null,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"Kt3XtqJC3omXLph7","img":"systems/pf2e/icons/default-icons/lore.svg","name":"Nature","sort":2200000,"system":{"description":{"value":""},"source":{"value":""},"rules":[],"slug":null,"schema":{"version":0.819,"lastMigration":null},"mod":{"value":10},"proficient":{"value":0},"t
рые <span class=\"Признак\">растительные</span> монстры, скорее всего, будут немногословны и уклончивы, в то время как менее умные часто говорят что-то бессмысленное.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"rules":[],"slug":"speak-with-plants","schema":{"version":0.819,"lastMigration":null},"traits":{"value":["plant"],"rarity":"common","custom":""},"level":{"value":4},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["primal"]},"school":{"value":"divination"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":null,"time":{"value":"2"},"duration":{"value":"10 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"VtOvwAguc0r46tbG","heightenedLevel":4},"hasCounteractCheck":{"value":false},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"type":"spell","effects":[],"folder":null,"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"pMSeU5bKC5Lsck6O","img":"systems/pf2e/icons/default-icons/melee.svg","name":"Кулак","sort":300000,"system":{"description":{"value":""},"source":{"value":""},"rules":[],"slug":null,"schema":{"version":0.819,"lastMigration":null},"traits":{"value":["agile","finesse"],"custom":"","rarity":"common"},"weaponType":{"value":"melee"},"attack":{"value":""},"damageRolls":{"nk5cjthg7seu83chpp3q":{"damage":"1d6+4","damageType":"bludgeoning"}},"bonus":{"value":10},"attackEffects":{"value":["grab"],"custom":""}},"type":"melee","effects":[],"folder":null,"ownership":{"default":0},"flags":{"babele":{"translated":true,"hasTranslation":true,"originalName":"Fist"}},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":nul